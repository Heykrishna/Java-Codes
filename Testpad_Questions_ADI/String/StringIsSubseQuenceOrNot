

class Result {

    static boolean strSubsequence(String str1, String str2) {
        int j = 0;
        for (int i = 0; i < str1.length(); i++) {
            if (str1.charAt(i) == str2.charAt(j)) {
                j++;
                if (j == str2.length()) {
                    return true;
                }
            }
        }
        return false;
    }
}
